<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LKCB Mapping</name>
    <description>
        Chris Banks and Lucas Kaastra's Map Navigation Tool. With our app you can find where you are going at school
        and locate various labs.
    </description>
    <author email="cbanks4667@conestogac.on.ca" href="http://christopher.banks.website">
       Chris Banks
       Lucas Kaastra
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="Icon.png"/>
    <splash src="Splash.png"/>
</widget>
